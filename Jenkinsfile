pipeline {
    agent any
    stages {
        stage('Cert Stage 1') {
            steps {
                echo 'This is the first stage of my Certification Pipeline'
            }
        }
        stage('Cert Stage 2') {
            steps {
                echo 'This is the second stage of my Certification Pipeline'
            }
        }
        stage('Cert Stage 3') {
            steps {
                echo 'This is the third and final stage of my Certification Pipeline'
            }
        }            
    }
}
